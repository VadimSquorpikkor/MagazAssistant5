<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.squorpikkor.magazassistant5">

    <application
        android:name=".ui.main.App"
        android:allowBackup="true"
        android:icon="@drawable/magaz_assistant"
        android:label="@string/app_name"
        android:roundIcon="@drawable/magaz_assistant"
        android:supportsRtl="true"
        android:theme="@style/Theme.MagazAssistant5">
        <activity
            android:name=".ui.main.pager.InfoActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MagazAssistant5">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>